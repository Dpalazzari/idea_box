<h1>All Users</h1>

  <div class='pages'>
    <%= paginate @users %>
  </div>
  <div class='container'>
    <% @users.each do |user| %>
      <div class='col-md-4'>
      <%= link_to user.name, user_path(user) %><br>
        <%= link_to 'Delete', admin_user_path(user), method: :delete, class:'btn btn-danger btn-xs'%><br><br>
      </div>
    <% end %>
  </div>
